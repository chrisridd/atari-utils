# const Fonthead fnt_st_6x6 = {
#     1,                  /*   WORD font_id       */
#     8,                  /*   WORD point         */
#     "6x6 system font",  /*   char name[32]      */
#     0,                  /*   UWORD first_ade    */
#     255,                /*   UWORD last_ade     */
#     4,                  /*   UWORD top          */
#     4,                  /*   UWORD ascent       */
#     3,                  /*   UWORD half         */
#     1,                  /*   UWORD descent      */
#     1,                  /*   UWORD bottom       */
#     5,                  /*   UWORD max_char_width*/
#     6,                  /*   UWORD max_cell_width*/
#     0,                  /*   UWORD left_offset  */
#     3,                  /*   UWORD right_offset */
#     1,                  /*   UWORD thicken      */
#     1,                  /*   UWORD ul_size      */
#     0x5555,             /*   UWORD lighten      */
#     0xAAAA,             /*   UWORD skew         */
#     F_STDFORM | F_MONOSPACE,/*   UWORD flags        */
# 
#     0,                  /*   UBYTE *hor_table   */
#     off_6x6_table,      /*   UWORD *off_table   */
#     dat_table,          /*   UWORD *dat_table   */
#     192,                /*   UWORD form_width   */
#     6,                  /*   UWORD form_height  */
# 
#     0,                  /*   Fonthead * next_font    */
#     0                   /*   reserved by Atari  */
# };


# const Fonthead fnt_st_8x8 = {
#     1,  /* WORD font_id */
#     9,  /* WORD point */
#     "8x8 system font",  /*   char name[32]      */
#     0,  /* WORD first_ade */
#     255,  /* WORD last_ade */
#     6,  /* UWORD top */
#     6,  /* UWORD ascent */
#     4,  /* UWORD half */
#     1,  /* UWORD descent */
#     1,  /* UWORD bottom */
#     7,  /* UWORD max_char_width */
#     8,  /* UWORD max_cell_width */
#     1,  /* UWORD left_offset */
#     3,  /* UWORD right_offset */
#     1,  /* UWORD thicken */
#     1,  /* UWORD ul_size */
#     0x5555, /* UWORD lighten */
#     0x5555, /* UWORD skew */
#     F_STDFORM | F_MONOSPACE | F_DEFAULT,  /* UWORD flags        */
#     0,                  /*   UBYTE *hor_table   */
#     off_8x8_table,       /*   UWORD *off_table   */
#     dat_table,           /*   UWORD *dat_table   */
#     256,  /* UWORD form_width */
#     8,  /* UWORD form_height */
#     0,  /* Fonthead * next_font */
#     0                   /*   reserved by Atari  */
# };

sub extract_bitmap
{
	my ($font, $formtable, $formw, $formh, $bitmap) = @_;
	foreach my $i (0..$formh - 1) {
		my $start = 'x' x ($formtable + $i * $formw);
		my (@w) = unpack($start . "n" x ($formw / 2), $font);
		my $row = '';
		foreach my $word (@w) {
			$row .= sprintf "%016b", $word;
		}
		@{$bitmap}[$i] = $row;
	}
}

sub extract_chartable
{
	my ($font, $chartable, $formw, $formh, $first, $last, $table) = @_;
	my (@offsets) = unpack("x$chartable" . "v" x ($last - $first + 1), $font);
	@{$table} = @offsets;
}

sub getcharindex
{
	my ($char, $bitmap, $chartable) = @_;
	my $start = $chartable->[$char];
	my $endpos = $chartable->[$char + 1];
	my $end;
	if (!defined($endpos)) {
		$end = undef;
	} else {
		$end = $endpos - 1;
	}
	my @new;
	foreach my $row (@{$bitmap}) {
		if (defined($end)) {
			push @new, substr $row, $start, $end - $start + 1;
		} else {
			push @new, substr $row, $start;
		}
	}
	return @new;
}

# eg set leftspace = 0 and rightspace = 1
sub autotrim
{
	my ($leftspace, $rightspace, $bits) = @_;
	# Trim all left bits
	my $set = 0;
	while ($set != 1) {
		foreach my $row (@{$bits}) {
			unless ($row =~ m{.}) {
				$set = 1;
				last;
			}
			$set = 1 if $row =~ m{^1};
		}
		if ($set == 0) {
			foreach my $row (@{$bits}) {
				$row =~ s{^.}{};
			}
		}
	}
	# Trim all right bits
	$set = 0;
	while ($set != 1) {
		foreach my $row (@{$bits}) {
			unless ($row =~ m{.}) {
				$set = 1;
				last;
			}
			$set = 1 if $row =~ m{1$};
		}
		if ($set == 0) {
			foreach my $row (@{$bits}) {
				$row =~ s{.$}{};
			}
		}	
	}
	# Add left space
	while ($leftspace > 0) {
		foreach my $row (@{$bits}) {
			$row =~ s{^}{0};
		}
		$leftspace--;
	}
	# Add right space
	while ($rightspace > 0) {
		foreach my $row (@{$bits}) {
			$row =~ s{$}{0};
		}
		$rightspace--;
	}
}

sub appendchar
{
	my ($bits, $newbitmap, $chartable) = @_;
	push @{$chartable}, length @{$newbitmap}[0];
	foreach my $i (0..scalar(@{$bits}-1)) {
		@{$newbitmap}[$i] .= @{$bits}[$i];
	}
}

sub padtowords
{
	my ($bitmap) = @_;
	my $needed = 16 - (length(@{$bitmap}[0]) % 16);
	foreach my $row (@{$bitmap}) {
		$row .= "0" x $needed;
	}
}

sub generate
{
	my %arg = @_;
	my $hex = $arg{form};
	my $formw = $arg{formw};
	my $formh = $arg{formh};
	my $monow = $formw * 8 / 256;

	my @originalbitmap;
	my @originalchartable;
	my @newbitmap;
	my @newchartable;
	my $wcell = 0;
	my $wchar = 0;

	$hex =~ s/[ \r\n]+//g;
	my $tmp = pack 'H*', $hex;
	extract_bitmap($tmp, 0, $formw, $formh, \@originalbitmap);

	# Fake up the chartable
	my $offset = 0;
	foreach my $i (0..255) {
		push @originalchartable, $offset;
		$offset += $monow;
	}
	push @originalchartable, $offset;

	foreach my $i (0..scalar$#originalbitmap) {
		$newbitmap[$i] = '';
	}

	foreach my $i (0..255) {
		my @new = getcharindex($i, \@originalbitmap, \@originalchartable);
		autotrim($arg{leftpadding}, $arg{rightpadding}, \@new) if $i > 32;
		$wcell = length($new[0]) if length($new[0]) > $wcell;
		appendchar(\@new, \@newbitmap, \@newchartable);
		# Bit of extra work, only to calculate the maximum char width
		autotrim(0, 0, \@new);
		$wchar = length($new[0]) if length($new[0]) > $wchar;
	}
	# Compendium/toshyp says an extra entry is added
	push @newchartable, length $newbitmap[0];

	padtowords(\@newbitmap);

	my $h = '';
	$h .= pack("vva32", $arg{fontid}, $arg{points}, "Sunnyvale");
	$h .= pack("vv", 0, 255);
	$h .= pack("vvvvv", $arg{top}, $arg{ascent}, $arg{half}, $arg{descent}, $arg{bottom}); #13, 11, 8, 2, 2);
	$h .= pack("vv", $wchar, $wcell);
	$h .= pack("vv", 0, 0);
	$h .= pack("vv", $arg{thicken}, $arg{underline}); #2, 2);
	$h .= pack("vv", $arg{lighten}, $arg{skew}); #0x5555, 0x5555); # 0x2222 better for skewing?
	$h .= pack("v", 0); # flags
	$h .= pack("VV", 0x0058, 0x0058);
	$h .= pack("V", 0x58 + (255-0+2) * 2);
	$h .= pack("vv", length($newbitmap[0])/8, ($#newbitmap + 1));
	$h .= pack("V", 0);
	print STDERR "Start of chartable: " . sprintf("%08x", length($h)) . "\n";
	$h .= pack("v*", @newchartable);
	print STDERR "Start of font data: " . sprintf("%08x", length($h)) . "\n";
	foreach my $row (@newbitmap) {
		$h .= pack("B*", $row);
	}
	open my $fh, '>', $arg{filename} or die;
	print $fh $h;
	close $fh;
}

generate(
	form => $hex8x16, formw => 256, formh => 16,
	leftpadding => 0, rightpadding => 2,
	fontid => 42, name => 'Sunnyvale',
	points => 10,
	top => 13, ascent => 11, half => 8, descent => 2, bottom => 2,
	thicken => 2, underline => 2,
	lighten => 0x5555, skew => 0x5555,
	filename => "FMSV10.FNT"
);

generate(
	form => $hex8x8, formw => 256, formh => 8,
	leftpadding => 0, rightpadding => 2,
	fontid => 42, name => 'Sunnyvale',
	points => 9,
	top => 6, ascent => 6, half => 4, descent => 1, bottom => 1,
	thicken => 1, underline => 1,
	lighten => 0x5555, skew => 0x5555,
	filename => "FMSV09.FNT"
);

generate(
	form => $hex6x6, formw => 192, formh => 6,
	leftpadding => 0, rightpadding => 1,
	fontid => 42, name => 'Sunnyvale',
	points => 8,
	top => 4, ascent => 4, half => 3, descent => 1, bottom => 1,
	thicken => 1, underline => 1,
	lighten => 0x5555, skew => 0xaaaa,
	filename => "FMSV08.FNT"
);

exit 0;
